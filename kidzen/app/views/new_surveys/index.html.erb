<h1>Listing new_surveys</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @new_surveys.each do |new_survey| %>
      <tr>
        <td><%= new_survey.name %></td>
        <td><%= new_survey.user_id %></td>
        <td><%= link_to 'Show', new_survey %></td>
        <td><%= link_to 'Edit', edit_new_survey_path(new_survey) %></td>
        <td><%= link_to 'Destroy', new_survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New New survey', new_new_survey_path %>
