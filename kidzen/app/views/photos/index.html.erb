
<% for photo in @photos %>
<div class="photo"> 
Description: <%= photo.description %> 
<%= link_to "Show", photo %>
<%= link_to "Edit", edit_photo_path(photo) %>
<%= link_to "Destroy", photo, :confirm => 'Are you sure?', :method => :delete %> 
<br>
</div>
<% end %>

<p><%= link_to "New Photo", new_photo_path %></p>
